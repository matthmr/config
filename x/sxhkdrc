#### term-mode

super + control + x; Return; shift + u
  urxvt -e zsh
super + control + x; Return; shift + s
  st -e zsh
super + control + x; Return; shift + x
  xterm -e zsh
super + control + x; Return; u
  urxvt -e tmux new-session -A
super + control + x; Return; s
  st -e tmux new-session -A
super + control + x; Return; x
  xterm -e tmux new-session -A

#### action-mode

super + control + x; a; s
  doshutdown -x
super + control + x; a; r
  doreboot -x
super + control + x; a; m
  dosuspend -x
super + control + x; a; d
  dohibernate -x
super + control + x; a; l
  i3lock -i @SXHKDRC_IMAGE@ -e -f -b -n -t -p default

#### launch-mode

super + control + x; l; a
  alsa-dmenu
super + control + x; l; b
  ungoogled-chromium
super + control + x; l; f
  firefox
super + control + x; l; m
  mpc-dmenu
super + control + x; l; s
  screenshot-dir
super + control + x; l; Q
  screenshot-quick-dir
super + control + x; l; q
  screenshot-quick
super + control + x; l; j
  japanese-dmenu
super + control + x; l; l
  urxvt -e lf -single
super + control + x; l; e
  emacstc
super + control + x; l; x
  emacs-x

#### misc

super + p
	dmenu_run
super + c
  clipmenu
super + control + x; x; k
  xkill
super + control + x; x; r
  @SXHKDRC_RESIZE@
super + control + x; x; l
  @SXHKDRC_RELOAD@
